{
  "name": "djs-ts-bot-template",
  "version": "1.0.0",
  "description": "A discord bot template using discord.js and typescript",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register --files src/index.ts",
    "prebuild": "rimraf build",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node build/index.js"
  },
  "author": "DanPlayz0",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "discord.js": "^14.15.2",
    "dotenv": "^16.4.5",
    "mongoose": "^8.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.12.4",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4"
  }
}
